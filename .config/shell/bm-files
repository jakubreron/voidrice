# bookmarks
cfbf	    $XDG_CONFIG_HOME/shell/bm-files	# This file, a list of bookmarked files
cfbd	    $XDG_CONFIG_HOME/shell/bm-dirs		# A list of bookmarked directories similar to this file
cfbw	    $DOTFILES_DIR/universal/.config/bm-websites       # A list of boookmarked websites

# scripts
cfdb	    $XDG_SRC_HOME/dwmblocks/config.h
cfdw	    $XDG_SRC_HOME/dwm/config.h
cfdm	    $XDG_SRC_HOME/dmenu/config.h
cfst	    $XDG_SRC_HOME/st/config.h

# system
cfz	      $ZDOTDIR/.zshrc
cfa	      $XDG_CONFIG_HOME/shell/aliasrc
cfp	      $XDG_CONFIG_HOME/shell/profile
cfc	      $XDG_CONFIG_HOME/picom.conf
cfx	      $XDG_CONFIG_HOME/x11/xprofile
cfl	      $XDG_CONFIG_HOME/lf/lfrc
cft       $XDG_CONFIG_HOME/tmux/tmux.conf
cfg	      $DOTFILES_DIR/universal/.config/git/config
cfgrub    /etc/default/grub

