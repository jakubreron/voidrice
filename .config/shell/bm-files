# cf = config prefix
cfbf	${XDG_CONFIG_HOME:-$HOME/.config}/shell/bm-files	# This file, a list of bookmarked files
cfbd	${XDG_CONFIG_HOME:-$HOME/.config}/shell/bm-dirs		# A list of bookmarked directories similar to this file

cfdb	~/.local/src/dwmblocks/config.h
cfdw	~/.local/src/dwm/config.h
cfdm	~/.local/src/dmenu/config.h
cfst	~/.local/src/st/config.h

# system
cfz	$ZDOTDIR/.zshrc						                        # zsh (shell) config
cfa	${XDG_CONFIG_HOME:-$HOME/.config}/shell/aliasrc		# aliases used by zsh (and potentially other shells)
cfp	${XDG_CONFIG_HOME:-$HOME/.config}/shell/profile		# profile file for login settings for zsh
cfc	${XDG_CONFIG_HOME:-$HOME/.config}/picom.conf  		# composition manager
cfx	${XDG_CONFIG_HOME:-$HOME/.config}/x11/xprofile		# x11 profile init settings
cfl	${XDG_CONFIG_HOME:-$HOME/.config}/lf/lfrc	      	# lf (file browser) config
cfG /etc/default/grub

# apps
cflv ${XDG_CONFIG_HOME:-$HOME/.config}/lvim/config.lua # lunarvim
cfnbu	${XDG_CONFIG_HOME:-$HOME/.config}/newsboat/urls	 # RSS urls for newsboat

## personal configs
cft	~/.ticker.yaml # stonks
cfb	${XDG_CONFIG_HOME:-$HOME/.config}/personal/universal/.config/bookmarks # website bookmarks
cfg	${XDG_CONFIG_HOME:-$HOME/.config}/personal/universal/.config/git/config # gitconfig

# documentation
cfpar ${XDG_CONFIG_HOME:-$HOME/.config}/personal/arch/README.md
cfpur ${XDG_CONFIG_HOME:-$HOME/.config}/personal/universal/README.md
